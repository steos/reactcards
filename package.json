    "jsdom": "^9.1.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mocha": "^2.4.5",
    "raw-loader": "^0.5.1",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.1",
    "tape": "^4.5.1",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "chai": "^3.5.0",
